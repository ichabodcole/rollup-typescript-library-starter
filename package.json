{
  "name": "typescript-rollup-library-starter",
  "version": "1.0.0",
  "description": "Scaffolding for starting a rollup based typescript code library",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run clean && rollup -cw ./config/rollup.config.js",
    "build": "npm run clean && rollup -c ./config/rollup.config.js",
    "test": "jest --passWithNoTests --config jest.config.js",
    "test:dev": "jest --config jest.config.js --watch",
    "clean": "rimraf ./dist",
    "examples": "npm run clean:examples && rollup -c ./config/rollup.config.examples.js -w",
    "clean:examples": "rimraf ./examples/dist",
    "format": "prettier --write 'src/**/*.ts' 'examples/**/*.{j,t}s?(x)' ",
    "tsc": "tsc --noEmit",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx ./src ./examples",
    "prepare": "npm run build",
    "prepublish": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "directories": {
    "src": "src",
    "__test__": "__test__"
  },
  "keywords": [
    "starter"
  ],
  "author": "Cole Reed",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/types": "^7.10.3",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^13.0.1",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-typescript": "^5.0.1",
    "@types/jest": "^26.0.3",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.3.1",
    "eslint-config-developit": "^1.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-preact": "^0.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "preact": "^10.4.8",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.21.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-serve": "^1.0.4",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-worker": "^1.2.0",
    "ts-jest": "^26.1.1",
    "tslib": "^2.0.0",
    "tslint-config-prettier": "^1.18.0",
    "ttypescript": "^1.5.12",
    "typescript": "^3.9.5"
  }
}